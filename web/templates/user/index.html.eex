<h1>Listing Users</h1>
  <table class="table">
  <%= for user <- @users do %> 
  <tr>
<td><%= render "user.html", user: user %></td>
<td><%= link "View", to: user_path(@conn, :show, user.id) %></td>
 <td><%= link "Delete User", to: user_path(@conn, :delete, user), method: :delete, data: [confirm: "Are you sure?"], class: "btn btn-danger btn-xs" %></td>
</tr>

<% end %> </table>
